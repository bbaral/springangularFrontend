{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
      "whitespace": false,
      "no-trailing-whitespace": false,
      "no-inferrable-types": false,
      "max-line-length": false,
      "quotemark": false,
      "comment-format": false,
      "prefer-const": false,
      "one-line": false,
      "eofline": false,
      "class-name": false
    }
}
